(ns phel-snake-tests\modules\color-test
  (:require phel-snake\modules\color :refer [color-green color-red])
  (:require phel\test :refer [deftest is]))

(deftest test-color-green
  (is (= "\033[0;32mhi\033[0m" (color-green "hi"))))

(deftest test-color-red
  (is (= "\033[31;31mhi\033[0m" (color-red "hi"))))
