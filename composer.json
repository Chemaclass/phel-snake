{
    "name": "chemaclass/phel-snake",
    "keywords": [
        "phel",
        "lisp",
        "functional",
        "language"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-readline": "*",
        "phel-lang/phel-lang": "dev-feature/main-ns-global-constant",
        "chemaclass/phel-cli-gui": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "5.4.x-dev"
    },
    "scripts": {
        "start": [
            "@compile",
            "@play"
        ],
        "compile": "vendor/bin/phel compile --no-cache --main-ns='phel-snake\\game'",
        "play": "php play.php",
        "test": "vendor/bin/phel test"
    },
    "autoload": {
        "psr-4": {
            "PhelGenerated\\": "src/PhelGenerated"
        }
    },
    "minimum-stability": "dev"
}
